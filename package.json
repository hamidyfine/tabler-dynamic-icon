{
  "name": "tabler-dynamic-icon",
  "private": false,
  "version": "1.2.5",
  "description": "A component to render tabler-icons dynamically in React applications.",
  "author": {
    "name": "Hamid Yaftian",
    "email": "hamid.yaftian@gmail.com",
    "url": "https://github.com/hamidyfine"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "tabler-icons",
    "dynamic-icons",
    "webfont",
    "svg",
    "icons"
  ],
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "style.css": "./dist/style.css",
  "sideEffects": [
    "*/*.css",
    "*/*.scss",
    "**/*.css",
    "**/*.scss"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint . --report-unused-disable-directives --max-warnings 0 --no-warn-ignored --fix",
    "lint:styles": "stylelint src/**/*.scss",
    "generate-icons-enums": "node scripts/extract-icons-enums.js",
    "generate-icons-classes": "node scripts/extract-icons-classes.js",
    "generate-icons-types": "npm run generate-icons-enums && npm run generate-icons-classes",
    "prepare": "husky",
    "release": "pnpm build && release-it"
  },
  "peerDependencies": {
    "@tabler/icons-react": "^3.34.1",
    "@tabler/icons-webfont": "^3.34.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@tabler/icons-webfont": "^3.36.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-reactify": "^1.1.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "release-it": "^19.2.2",
    "rollup": "^4.54.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^4.0.1",
    "sass-embedded": "^1.97.1",
    "stylelint": "^16.26.1",
    "stylelint-config-hudochenkov": "^12.0.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.13.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.36.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:scripts"
    ],
    "*.{css,scss}": [
      "npm run lint:styles"
    ]
  }
}
